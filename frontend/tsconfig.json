{
  "compilerOptions": {
    "target": "es6",
    "allowJs": true,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "pretty": true,
    "alwaysStrict": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "lib": [
      "es5",
      "es6",
      "dom"
    ]
  },
  "include": [
    "src"
  ]
}
